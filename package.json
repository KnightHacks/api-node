{
  "name": "@knighthacks/hackathon",
  "version": "0.1.0-dev",
  "description": "The hackathon backend node API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --exec ts-node ./src/index.ts",
    "start:prod": "tsc && node ./dist/index.js",
    "test": "jest ./src/__tests__",
    "lint": "eslint ./src",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KnightHacks/knighthacks-api-node-2021.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KnightHacks/knighthacks-api-node-2021/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/KnightHacks/knighthacks-api-node-2021#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/humps": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/node-fetch": "^2.5.12",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.30.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "humps": "^2.0.1",
    "node-fetch": "^2.6.0",
    "qs": "^6.10.1",
    "snakecase-keys": "^4.0.2"
  },
  "files": [
    "/dist"
  ]
}
